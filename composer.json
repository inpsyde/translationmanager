{
	"name" : "eurotext/translationmanager",
	"description" : "WordPress Plugin to connect to the Eurotext REST API.",
	"license" : "proprietary",
	"type" : "wordpress-plugin",
	"config" : {
		"sort-packages" : true
	},
	"authors" : [
		{
			"name" : "Inpsyde GmbH",
			"email" : "info@inpsyde.com",
			"homepage" : "https://inpsyde.com/en/"
		}
	],
	"repositories" : [
		{
			"type" : "vcs",
			"url" : "git@bitbucket.org:inpsyde/system-status.git"
		}
	],
	"require" : {
		"php" : ">=5.6",
		"brain/nonces" : "1.0.*",
		"inpsyde/system-status" : "dev-master",
		"pimple/pimple" : "^3.2"
	},
	"require-dev" : {
		"phpunit/phpunit" : "~5",
		"brain/monkey" : "2.*",
		"squizlabs/php_codesniffer" : "~3",
		"phpcompatibility/php-compatibility" : "~9",
		"wp-coding-standards/wpcs" : "~2"
	},
	"scripts": {
		"post-install-cmd": "\"vendor/bin/phpcs\" --config-set installed_paths vendor/phpcompatibility/php-compatibility,vendor/wp-coding-standards/wpcs",
		"post-update-cmd" : "\"vendor/bin/phpcs\" --config-set installed_paths vendor/phpcompatibility/php-compatibility,vendor/wp-coding-standards/wpcs"
	},
	"autoload" : {
		"psr-4" : {
			"Translationmanager\\" : "src/"
		}
	},
	"autoload-dev" : {
		"psr-4" : {
			"Translationmanager\\Tests\\" : "tests/php",
			"Translationmanager\\Tests\\Unit\\" : "tests/unit/src",
			"Translationmanager\\Tests\\Integration\\" : "tests/integration/src"
		}
	},
	"config" : {
		"platform" : {
			"php" : "5.6"
		}
	}
}
