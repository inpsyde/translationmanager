{
  "name": "eurotext/translationmanager",
  "description": "WordPress Plugin to connect to the Eurotext REST API.",
  "license": "proprietary",
  "type": "wordpress-plugin",
  "config": {
    "sort-packages": true
  },
  "autoload": {
    "files": [
      "bootstrap.php"
    ]
  },
  "require-dev": {
    "phpunit/phpunit": "~5",
    "brain/monkey": "2.*",
    "sourcerer-mike/wp-readme-validator": "dev-master",
    "squizlabs/php_codesniffer": "~2.7",
    "wimg/php-compatibility": "^7.0",
    "wp-coding-standards/wpcs": "dev-master"
  },
  "autoload-dev": {
    "psr-4": {
      "Tmwp\\Tests\\": "tests/unit/src"
    }
  }
}
